cks.map(async p => {\n    if (!packKeys.next().value.startsWith('world.')) return\n\n    const packContent = await p.getDocuments()\n    const items = packContent.filter(search)\n    if (items) {\n        items.forEach(i => console.log(`----> Compendium: ${p.collection} - Item: ${i.name}`))\n    }\n}))","folder":"1F3e0ZCRKTOyCJmZ","sort":0,"permission":{"default":0,"3UjVNZPJLHwgrlkI":3},"flags":{"combat-utility-belt":{"macroTrigger":""},"advanced-macros":{"runAsGM":false}}}
