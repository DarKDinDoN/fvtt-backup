eOrRoll .dialog-content .separator-title:not(:first-child) {\n            padding-top: 15px;\n        }\n\n        #DDChooseUseOrRoll .dialog-content form .form-fields input {\n            flex: auto;\n        }\n\n        #DDChooseUseOrRoll .radios {\n            flex-wrap: wrap;\n        }\n\n        #DDChooseUseOrRoll .radios input[type=\"radio\"] {\n            opacity: 0;\n            position: fixed;\n            width: 0;\n        }\n\n        #DDChooseUseOrRoll .radios label {\n            cursor: pointer;\n            display: flex;\n            flex: 1 !important;\n            margin: 2px;\n            width: 100%;\n            border-radius: 3px;\n            white-space: nowrap;\n            font-size: 13px;\n            justify-content: center;\n            align-items: center;\n            border-width: 9px;\n            background: rgba(0, 0, 0, 0.1);\n            border: 2px groove #f0f0e0;\n            padding: 0 10px;\n        }\n\n        #DDChooseUseOrRoll .radios label i {\n            margin-right: 5px;\n        }\n\n        #DDChooseUseOrRoll .radios label:hover {\n            box-shadow: 0 0 8px white;\n        }\n        \n        #DDChooseUseOrRoll .radios input[type=\"radio\"]:checked + label {\n            background: rgba(0, 0, 0, 0.2);\n        }\n\n        #DDChooseUseOrRoll .radios input[type=\"radio\"]:disabled + label {\n            color: #4b4a44;\n            box-shadow: none;\n            border-color: rgb(201, 199, 184);\n            background: rgba(0, 0, 0, 0.05);\n        }\n        </style>\n\n        <div class=\"header\">\n            <img src=\"${item.data.img}\" width=\"36\" height=\"36\"/>&nbsp;&nbsp;&nbsp;\n            <div>${item.data.name}</div>\n        </div>\n\n        <div class=\"question\">Quel sort souhaitez-vous améliorer ?</div>\n\n        <form>\n            <div class=\"form-group\">\n                <div class=\"form-fields buttons radios\">\n                    ${(function () {\n            let radios = ''\n\n            const spells = actor.items.filter(\n                i => i.data.type === 'spell' &&\n                    i.data.data.preparation?.mode === 'prepared' &&\n                    i.data.data.preparation?.prepared &&\n                    i.data.data.level >= 1 &&\n                    i.data.data.damage?.parts?.every(p => p[1] !== 'healing')\n            )\n\n            spells.sort((a, b) => {\n                return a.data.data.level >= b.data.data.level ? 1 : -1\n            })\n\n            const spellSlots = Object.entries(actor.data.data.spells).filter(s => (s[1].max > 0 && s[0] !== 'pact'))\n            const firstSpellSlotAvailable = spellSlots.findIndex(s => s[1].value > 0)\n\n            let checked = false\n            let alreadyChecked = false\n\n            for (let index = 0; index < spells.length; index++) {\n                if (index === 0 || spells[index].data.data.level !== spells[index - 1].data.data.level) radios += `<div class=\"separator-title\">Sorts de niveau ${spells[index].data.data.level}</div>`\n\n                if (spells[index].data.data.level - 1 === firstSpellSlotAvailable && !alreadyChecked) {\n                    checked = true\n                    alreadyChecked = true\n                }\n\n                radios += `\n                                <input type=\"radio\" name=\"spell\" id=\"${spells[index].data._id}\" value=\"${spells[index].data._id}\" ${checked ? 'checked' : ''} ${spellSlots[spells[index].data.data.level - 1][1].value < 1 ? 'disabled' : ''}>\n                                <label for=\"${spells[index].data._id}\">\n                                    <img src=\"${spells[index].img}\" width=\"16px\" height=\"16px\"> &nbsp;\n                                    ${spells[index].data.name}\n                                </label>\n                            `\n\n                checked = false\n            }\n\n            return radios\n        }())}\n                </div>\n            </div>\n        </form>\n\n        <script>\n            if (${itemUse.data.data.uses.value} < 1) {\n                document.querySelector(\"button.use\").disabled = true\n            }\n        </script>\n    `,\n    buttons: {\n        use: {\n            icon: \"<i class='fas fa-dot-circle'></i>\",\n            label: `${itemUse.data.name.slice(0, 19).concat('...')} (${itemUse.data.data.uses.value})`,\n            callback: () => {\n                applyChanges = true\n                mode = 'use'\n            }\n        },\n        roll: {\n            icon: \"<i class='fas fa-dice-six'></i>\",\n            label: `${rollFormula} ${rollLabel}`,\n            callback: () => {\n                applyChanges = true\n                mode = 'roll'\n            }\n        }\n    },\n    default: itemUse.data.data.uses.value > 0 ? \"use\" : \"roll\",\n    close: async html => {\n        if (applyChanges) {\n            let spellId = html.find('[name=\"spell\"]:checked').val()\n            if (!spellId) {\n                return ui.notifications.warn('You need to select a spell.')\n            }\n\n            let roll\n            let label\n            let spell = actor.items.get(spellId)\n            let spellLevel = spell.data.data.level\n\n            if (mode === 'use') {\n                if (itemUse.data.data.uses.value < 1) {\n                    return ui.notifications.warn(`Not enought charges on item ${'Canalisation d\\’énergie divine'}.`)\n                }\n                itemUse.update({ 'data.uses.value': itemUse.data.data.uses.value - 1 })\n                roll = await new Roll('1').roll({ async: true })\n                label = itemUse.data.name\n            } else if (mode === 'roll') {\n                roll = await new Roll(rollFormula).roll({ async: true })\n                game.dice3d.showForRoll(roll, game.user, true)\n                label = rollLabel.charAt(0).toUpperCase() + rollLabel.slice(1)\n            }\n\n            const spellKey = `data.spells.spell${spellLevel}.value`\n            await actor.update({ [spellKey]: actor.data.data.spells[`spell${spellLevel}`].value - 1 })\n\n            await game.macros.getName('itemToChat').execute(actor, item, { configureDialog: false })\n\n            const messageData = {\n                whisper: [],\n                user: game.user.data._id,\n                speaker: ChatMessage.getSpeaker({ actor }),\n                flavor: `${item.data.name} - résultat`,\n                content: `\n                    <div class=\"dice-roll\">\n                        <div class=\"dice-result\">\n                            <h4 class=\"dice-total\" style=\"flex: 1; margin-bottom: 0.4rem;\">\n                                ${roll.result}\n                                <div style=\"margin-bottom: 0.25rem;font-size: .8rem;line-height: .8rem;font-weight: normal;\">${label}</div>\n                            </h4>\n                        </div>\n                    </div>\n                    <hr>\n                    <div class=\"dice-roll\">\n                        <div class=\"dice-result\">\n                            <h4 class=\"dice-total\" style=\"flex: 1;\">\n                                ${spellLevel + 1}\n                                <div style=\"margin-bottom: 0.25rem;font-size: .8rem;line-height: .8rem;font-weight: normal;\">Niveau du sort à lancer<br>(sans consommer d'emplacement)</div>\n                            </h4>\n                        </div>\n                    </div>\n                `,\n            }\n\n            ChatMessage.create(messageData);\n\n            await game.macros.getName('itemToChat').execute(actor, spell)\n        }\n    }\n}, {\n    id: 'DDChooseUseOrRoll'\n}).render(true)","folder":null,"sort":0,"permission":{"default":0},"flags":{}}}},"autoanimations":{"killAnim":false,"below":false,"override":false,"targetToken":{"enable":false},"sourceToken":{"enable":false},"allSounds":{"item":{"enableAudio":false}},"autoOverride":{"enable":f