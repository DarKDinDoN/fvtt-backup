: center;\n            border-width: 9px;\n            background: rgba(0, 0, 0, 0.1);\n            border: 2px groove #f0f0e0;\n        }\n\n        #DDSpellSlotUse .radios label i {\n            margin-right: 5px;\n        }\n\n        #DDSpellSlotUse .radios label:hover {\n            box-shadow: 0 0 8px white;\n        }\n        \n        #DDSpellSlotUse .radios input[type=\"radio\"]:checked + label {\n            background: rgba(0, 0, 0, 0.2);\n        }\n\n        #DDSpellSlotUse .radios input[type=\"radio\"]:disabled + label {\n            color: #4b4a44;\n            box-shadow: none;\n            border-color: rgb(201, 199, 184);\n            background: rgba(0, 0, 0, 0.05);\n        }\n        </style>\n\n        <div class=\"header\">\n            <img src=\"${item.data.img}\" width=\"36\" height=\"36\"/>&nbsp;&nbsp;&nbsp;\n            <div>${item.data.name}</div>\n        </div>\n\n        <div class=\"question\">Quel niveau d'emplacement de sort ?</div>\n\n        <form>\n            <div class=\"form-group\">\n                <div class=\"form-fields buttons radios\">\n                    ${(function () {\n            let radios = ''\n            const spellSlots = Object.entries(actor.data.data.spells).filter(s => (s[1].max > 0 && s[0] !== 'pact'))\n            const firstSpellSlotAvailable = spellSlots.findIndex(s => s[1].value > 0)\n\n            for (let index = 1; index <= Math.min(spellSlots.length, 7); index++) {\n                radios += `\n                                <input type=\"radio\" name=\"level\" id=\"level${index}\" value=\"${index}\" ${spellSlots[index - 1][1].value < 1 ? 'disabled' : ''} ${firstSpellSlotAvailable === index - 1 ? 'checked' : ''}>\n                                <label for=\"level${index}\">Niveau ${index} (${spellSlots[index - 1][1].value})</label>\n                            `\n            }\n\n            return radios\n        }())}\n                </div>\n            </div>\n        </form>\n    `,\n    buttons: {\n        use: {\n            icon: '<i class=\"fas fa-times\"></i>',\n            label: `Annuler`\n        },\n        roll: {\n            icon: '<i class=\"fas fa-check\"></i>',\n            label: `Valider`,\n            callback: () => applyChanges = true\n        }\n    },\n    default: \"use\",\n    close: async html => {\n        if (applyChanges) {\n            let spellLevel = parseInt(html.find('[name=\"level\"]:checked').val())\n\n            if (!spellLevel) {\n                return ui.notifications.warn('You need to select a spell level.')\n            }\n\n            const spellKey = `data.spells.spell${spellLevel}.value`\n            await actor.update({ [spellKey]: actor.data.data.spells[`spell${spellLevel}`].value - 1 })\n\n            await game.macros.getName('itemToChat').execute(actor, item)\n\n            const messageData = {\n                whisper: [],\n                user: game.user.data._id,\n                speaker: ChatMessage.getSpeaker({ actor }),\n                flavor: `${item.data.name} - résultat`,\n                content: `\n                    <div class=\"dice-roll\">\n                        <div class=\"dice-result\">\n                            <h4 class=\"dice-total\" style=\"flex: 1;\">\n                            ${spellLevel}\n                            <div style=\"margin-bottom: 0.25rem;font-size: .8rem;line-height: .8rem;font-weight: normal;\">Niveau d'emplacement de sort</div>\n                            </h4>\n                        </div>\n                    </div>\n                `,\n            }\n\n            ChatMessage.create(messageData);\n        }\n    }\n}, {\n    id: 'DDSpellSlotUse'\n}).render(true)","folder":null,"sort":0,"permission":{"default":0},"flags":{}}}},"mre-dnd5e":{"formulaGroups":[{"label":"Primaire","formulaSet":[]}]},"autoanimations":{"killAnim":false,"below":false,"override":false,"targetToken":{"enable":false},"sourceToken":{"enable":false},"allSounds":{"item":{"enableAudio":false}},"autoOverride":{"enable":false},"version":2,"audio":{"a01":{"enable":false,"file":"","delay":0,"volume":0.25},"e01":{"enable":false,"file":"","delay":0,"volume":0.25}}}}}
