{"name":"Alien -  GM Dice Roller","permission":{"default":0,"vVRzTNcybBml63HZ":3,"tZxbgUAd9T0LLY0c":3},"type":"script","sort":100001,"flags":{"furnace":{"runAsGM":false}},"scope":"global","command":"(async () => {\nlet hostile = false;\nlet label = 'GM';\nlet reRoll = true;\n\n  let template = `\n    <form>\n        <div class=\"form-group\">\n            <label>How Many Base Die?</label>\n            <input type=\"text\" id=\"fr1Data\" value=1>\n        </div>\n        <div class=\"form-group\">\n            <label>How Many Stress Dice?</label>\n            <input type=\"text\" id=\"fr2Data\" value=0>\n        </div>\n\n        <div class=\"form-group\">\n            <label>GM Only</label>\n            <input type=\"checkbox\" id=\"fblind\" value=\"true\" checked>\n        </div>\n\n    </form>`;\n\n  let buttons = {};\n  // if (game.tables.entities.length > 0) {\n  buttons = {\n    draw: {\n      icon: '<i class=\"fas fa-check\"></i>',\n      label: 'Roll',\n      callback: async (html) => {\n        // const tableId = html.find('#tableSelect')[0].value\n        // const table = game.tables.get(tableId);\n        const r1Data = parseInt(html.find('#fr1Data')[0].value || 0);\n        const r2Data = parseInt(html.find('#fr2Data')[0].value || 0);\n        const blind = html.find('#fblind')[0].checked;\n\n        await game.alienrpg.yze.yzeRoll(hostile, blind, reRoll, label, r1Data, 'Black', r2Data, 'Stress');\n      },\n    },\n    cancel: {\n      icon: '<i class=\"fas fa-times\"></i>',\n      label: 'Cancel',\n    },\n  };\n\n  new Dialog({\n    title: 'Roll YZE Dice',\n    content: template,\n    buttons: buttons,\n    default: 'draw',\n  }).render(true);\n})();","author":"tZxbgUAd9T0LLY0c","img":"systems/alienrpg/ui/DsN/alien-dice-y6.png","actorIds":[],"_id":"6Im4aIiiSO11ESw1"}
