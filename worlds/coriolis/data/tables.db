"type":0,"weight":1,"range":[6,6],"text":"<b>NAME</b>: Yala <br /> <b>PROFESSION</b>: Smuggler <br /> <b>CHARACTERISTICS</b>: Baggy clothes <br /> <b>ACTIVITY</b>: Searching <br /> <b>MOTIVATION</b>: Birr! <br />","img":null,"documentCollection":"","documentId":null,"drawn":false,"flags":{}}],"replacement":true,"displayRoll":true,"formula":"d6","ownership":{"default":0,"GyrqPCYtbXkNshoX":3},"_stats":{"systemId":"yzecoriolis","systemVersion":"3.0.12","coreVersion":"10.291","createdTime":1680376286673,"modifiedTime":1680376286673,"lastModifiedBy":"GyrqPCYtbXkNshoX"},"folder":"XQ9tAfxtGl31AxFL","sort":0}
